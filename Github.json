    "rest": {
      "config": {
        "baseUrl": "https://api.github.com",
        "get": {
          "pagination": {
                "mode": "link",
                "page_size":"{page_size}"
          },
          "queryParameters": {
            "per_page": "{page_size}"
          }
        },
        "authentication": "authorization_code_grant",
        "call_handling": "generic",
        "test_connection": {
          "url": "/organizations"
        },
        "socket_timeout": 3600000,
        "retry": [
            {
                "type": "statusCode",
                "statusCode": 429,
                "retry": {
                    "type": "simple",
                    "maxCount": 12,
                    "waitTime": 5
                }
            },
            {
                "type": "statusCode",
                "statusCode": 500,
                "retry": {
                    "type": "simple",
                    "maxCount": 12,
                    "waitTime": 5
                }
            },
            {
                "type": "connectionError"
            }
        ]
      },
      "authOptions": {
            "type": "authorization_code_grant",
            "authUrl": "https://github.com/login/oauth/access_token",
            "loginUrl": "https://github.com/login/oauth/authorize?client_id={client_id}&redirect_uri={redirect_uri}&scope={scopes}",
            "scopes": [
                "repo"
            ]
      }
    },
    "connection": {
      "items": [
        {
          "name": "page_size",
          "type": "textbox",
          "label": "Page Size",
          "value": "100"
        },
        {
          "name": "organization",
          "type": "textbox",
          "label": "Organization",
          "value": ""
        }
      ]
    }
